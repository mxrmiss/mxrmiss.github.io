<div class="clearfix">
    <article id="page">
        <div class="page-content">
            <h1><%= page.title %></h1>
            <%- page.content %>
        </div>
        <% if (theme.about.enable && theme.about.comment && theme.valine.enable && theme.valine.appid && theme.valine.appkey) { %>
            <%- partial('partials/valine') %>
        <% } %>
    </article>
    <%- partial('partials/content-aside') %>
</div>
<%- paginator() %>
